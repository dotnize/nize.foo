{
  "tabWidth": 2,
  "semi": true,
  "printWidth": 100,
  "singleQuote": false,
  "trailingComma": "es5",
  "endOfLine": "lf",

  "plugins": [
    "prettier-plugin-astro",
    "prettier-plugin-organize-imports",
    "prettier-plugin-tailwindcss"
  ],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ]
}
